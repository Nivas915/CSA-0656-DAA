#include <stdio.h>
#include<string.h>
#define MAX 100

int main() {
    char str[MAX];
    int length = 0;
    printf("V.Laxmi Nivas - 192211694\n");
    printf("Enter a string: ");
    fgets(str, MAX, stdin);

    str[strcspn(str, "\n")] = 0;

    while (str[length] != '\0') {
        length++;
    }

    printf("The length of the string is: %d\n", length);

    return 0;
}
